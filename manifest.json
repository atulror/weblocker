{
  "manifest_version": 2,
  "name": "social media blocker",
  "description": "Simply Blocks popular social media website with one click!",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "icon128.png",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"]
    }
  ]
}
